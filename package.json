{
  "name": "6502-emulator",
  "version": "1.0.0",
  "description": "Emulator for the A.C. Wright 6502 project",
  "main": "index.js",
  "bin": {
    "ac6502": "./dist/index.js"
  },
  "scripts": {
    "build": "npx tsc",
    "start": "node ./dist/index.js",
    "woz": "node ./dist/index.js -p /dev/tty.usbserial-FTDMBHZ7 -r /Users/acwright/Developer/Assembly/6502-ASM/Wozmon/Wozmon.bin",
    "test": "NODE_OPTIONS='--no-experimental-webstorage' jest",
    "test:watch": "NODE_OPTIONS='--no-experimental-webstorage' jest --watch",
    "test:coverage": "NODE_OPTIONS='--no-experimental-webstorage' jest --coverage --coverageReporters=text-summary --coverageReporters=lcov"
  },
  "keywords": [
    "6502",
    "emulator",
    "8-bit",
    "sdl",
    "binary",
    "assembly",
    "typescript"
  ],
  "author": "A.C. Wright",
  "license": "MIT",
  "repository": "https://github.com/acwright/6502-Emulator",
  "homepage": "https://github.com/acwright/6502-Emulator/README.md",
  "type": "commonjs",
  "dependencies": {
    "@kmamal/sdl": "^0.11.13",
    "commander": "^14.0.3",
    "figlet": "^1.10.0",
    "serialport": "^13.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.3.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}
